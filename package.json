{
  "name": "vapi-call-logs",
  "version": "1.0.0",
  "description": "A Node.js server to fetch call logs from Vapi.ai",
  "main": "server.js",
  "type": "module",
  "scripts": {
   
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm install --prefix client && npm run build --prefix client",
    "install-server": "npm install",
    "install-client": "npm install --prefix client",
    "postinstall": "npm run install-client && npm run build"
  },
  "keywords": [
    "vapi",
    "api",
    "node",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^10.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
}
